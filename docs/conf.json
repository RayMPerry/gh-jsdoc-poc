{
  "opts": {
    "recurse": true
  },
  "plugins": [
    "node_modules/better-docs/typescript"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(j|t)s(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
